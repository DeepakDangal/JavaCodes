<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="606.0" prefWidth="515.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csd214deepak_dangal.Second">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label />
    <Label text="id" />
    <TextField fx:id="iid" />
   <Label text="Name" />
   <TextField fx:id="iname" />
   <Button mnemonicParsing="false" onAction="#UpdateClick" text="Update" />
   <Button mnemonicParsing="false" onAction="#DeleteClick" text="Delete" />
   <Button mnemonicParsing="false" onAction="#InsertClick" text="Insert" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="View" />
   <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="id" prefWidth="75.0" text="id" />
       <TableColumn fx:id="name" prefWidth="75.0" text="name" />
     </columns>
   </TableView>
</VBox>
